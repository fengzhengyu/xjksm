/**
 * Eric Meyer's Reset CSS v2.0 (http://meyerweb.com/eric/tools/css/reset/)
 * http://cssreset.com
 */



html 
 
  margin: 0
  padding: 0
  -webkit-text-size-adjust: 100%
  font-family: -apple-system, "PingFang SC", "Helvetica Neue", Helvetica, STHeiTi, sans-serif
  -webkit-tap-highlight-color: transparent
  background: #f3f4f6
  color: #000
  height: 100%
  font-size: 100px

body 
  font-family: Arial, "Microsoft Yahei", "Helvetica Neue", Helvetica, sans-serif
  font-size: 14px
  line-height: 1
  -webkit-text-size-adjust: none
  
  height: 100%
  -webkit-overflow-scrolling: touch
  width: 6.4rem;
  margin: 0 auto;
  padding: 0;

 div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td, hr, button, article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section, sumary 

  margin: 0;
  padding: 0;
  
article,aside,footer,header,section,nav,figure,figcaption,hgroup,details,menu
  display:block


input, select, textarea 

  border: 0
  outline: none

ol, ul 

  list-style: none

*:not(input, select, textArea) 

  -webkit-user-select: none


fieldset,img
  border:0
img,object,input,textarea,button,select
  vertical-align:middle



a, a:active, a:hover 

  text-decoration: none



a, button, input, textarea 

  -webkit-tap-highlight-color: rgba(0, 0, 0, 0)



